{
  "manifest_version": 3,
  "name": "Timer Extension",
  "version": "1.0",
  "description": "Timer Extension",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "24": "icon.png",
      "32": "icon.png"
    },
    "default_title": "Setup a timer"
  },
  "options_page": "options.html",
  "permissions": ["alarms", "storage", "notifications"],
  "background": {
    "service_worker": "background.js"
  }
}
